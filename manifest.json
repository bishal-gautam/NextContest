{
  "manifest_version": 2,
  "name": "Bishal Chrome Extension",
  "description": "Quick launch to Bishal Extension",
  "version": "1.0",
  "icons":{"128": "icon128.png"},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon19.png",
	"default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}